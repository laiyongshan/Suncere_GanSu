<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialCalendarView"><attr format="reference" name="mcv_dateTextAppearance"/><attr format="reference" name="mcv_weekDayTextAppearance"/><attr format="reference" name="mcv_headerTextAppearance"/><attr format="color" name="mcv_arrowColor"/><attr format="reference" name="mcv_leftArrowMask"/><attr format="reference" name="mcv_rightArrowMask"/><attr format="color" name="mcv_selectionColor"/><attr format="integer" name="mcv_showOtherDates">
            <flag name="none" value="0"/>
            <flag name="other_months" value="1"/>
            <flag name="out_of_range" value="2"/>
            <flag name="decorated_disabled" value="4"/>
            <flag name="defaults" value="4"/>
            <flag name="all" value="7"/>
        </attr><attr format="boolean" name="mcv_allowClickDaysOutsideCurrentMonth"/><attr format="reference" name="mcv_weekDayLabels"/><attr format="reference" name="mcv_monthLabels"/><attr format="dimension" name="mcv_tileSize">
            <enum name="match_parent" value="-1"/>
        </attr><attr format="dimension" name="mcv_tileHeight">
            <enum name="match_parent" value="-1"/>
        </attr><attr format="dimension" name="mcv_tileWidth">
            <enum name="match_parent" value="-1"/>
        </attr><attr format="enum" name="mcv_firstDayOfWeek">
            <enum name="sunday" value="1"/>
            <enum name="monday" value="2"/>
            <enum name="tuesday" value="3"/>
            <enum name="wednesday" value="4"/>
            <enum name="thursday" value="5"/>
            <enum name="friday" value="6"/>
            <enum name="saturday" value="7"/>
        </attr><attr format="enum" name="mcv_calendarMode">
            <enum name="month" value="0"/>
            <enum name="week" value="1"/>
        </attr><attr format="enum" name="mcv_titleAnimationOrientation">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="calendar_size_small">100dp</dimen>
    <dimen name="tile_size_medium">32dp</dimen>
    <dimen name="tile_size_small">16dp</dimen>
    <item name="mcv_pager" type="id"/>
    <string name="action_settings">Settings</string>
    <string name="app_name">TestAPP</string>
    <string name="calendar">Calendar</string>
    <string name="hello_world">Hello world!</string>
    <string name="next">Go to next</string>
    <string name="previous">Go to previous</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="HJOData_CustomTextAppearance" parent="TextAppearance.MaterialCalendarWidget.Date">
    <item name="android:textColor">@color/mcv_text_date_dark</item>
</style>
    <style name="TextAppearance.MaterialCalendarWidget.Date" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textSize">14sp</item>
        
        <item name="android:textColor">@color/mcv_text_date_light</item>
    </style>
    <style name="TextAppearance.MaterialCalendarWidget.Header" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextAppearance.MaterialCalendarWidget.WeekDay" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/mcv_text_week_color</item>
    </style>
</resources>